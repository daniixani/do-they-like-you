stdout.put(next, " ", nl);
add(1, next);

stdout.put( start, " " );
stdout.put( next, " " );  
add( 1, next );
stdout.put( next );   
add( 1, next );
stdout.newln();

//row 4
stdout.put( start, " " );
stdout.put( next, " " ); 
